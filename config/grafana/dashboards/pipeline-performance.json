            "refId": "B"
          },
          {
            "expr": "histogram_quantile(0.99, rate(cdc_polling_duration_seconds_bucket{service=\"producer\"}[5m]))",
            "legendFormat": "P99",
            "refId": "C"
          }
        ],
        "yaxes": [
          {"format": "s", "label": "Lat√™ncia"},
          {"format": "short"}
        ]
      },
      
      {
        "id": 6,
        "title": "‚è±Ô∏è Lat√™ncia de Valida√ß√£o ao Longo do Tempo",
        "type": "graph",
        "gridPos": {"x": 12, "y": 12, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "histogram_quantile(0.50, rate(validation_duration_seconds_bucket{service=\"consumer\"}[5m]))",
            "legendFormat": "P50",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.95, rate(validation_duration_seconds_bucket{service=\"consumer\"}[5m]))",
            "legendFormat": "P95",
            "refId": "B"
          },
          {
            "expr": "histogram_quantile(0.99, rate(validation_duration_seconds_bucket{service=\"consumer\"}[5m]))",
            "legendFormat": "P99",
            "refId": "C"
          }
        ],
        "yaxes": [
          {"format": "s", "label": "Lat√™ncia"},
          {"format": "short"}
        ]
      },
      
      {
        "id": 7,
        "title": "üíæ Uso de Mem√≥ria Heap (bytes)",
        "type": "graph",
        "gridPos": {"x": 0, "y": 20, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "jvm_memory_used_bytes{area=\"heap\",job=\"producer-service\"}",
            "legendFormat": "Producer Used",
            "refId": "A"
          },
          {
            "expr": "jvm_memory_max_bytes{area=\"heap\",job=\"producer-service\"}",
            "legendFormat": "Producer Max",
            "refId": "B"
          },
          {
            "expr": "jvm_memory_used_bytes{area=\"heap\",job=\"consumer-service\"}",
