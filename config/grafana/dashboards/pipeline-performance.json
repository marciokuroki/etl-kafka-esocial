      },
      
      {
        "id": 8,
        "title": "üóëÔ∏è Garbage Collection (GC)",
        "type": "graph",
        "gridPos": {"x": 12, "y": 20, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "rate(jvm_gc_pause_seconds_sum{job=\"producer-service\"}[5m])",
